        <!--
          - #SCRIPT PRODDUCTOS
        -->
        @model List<GamarraPlus.Models.Producto>

        <div class="py-2">
            <div class="container px-2 px-lg-2 mt-5">
                @if (Model != null && Model.Count > 0)
                {
                    <div id="catalogo-productos" class="row gx-4 gx-lg-5 row-cols-2 row-cols-md-3 row-cols-xl-4 justify-content-center">

                        @foreach (var producto in Model)
                        {
                            <div class="col mb-5">
                                <div class="card h-100">
                                    <img class="card-img-top" src="@producto.RutaImagen" alt="Imagen del producto">
                                    <div class="card-body p-4">
                                        <div class="text-center">
                                            <h5 class="fw-bolder">@producto.Descripcion</h5>
                                            <p>S/. @producto.PrecioVenta.ToString("F2")</p>
                                        </div>
                                    </div>
                                    <div class="card-footer p-4 pt-0 border-top-0 bg-transparent">
                                        <div class="d-grid d-md-grid gap-2 d-md-block align-items-center text-center">
                                            <button class="btn btn-primary">Agregar al carrito</button>
                                            <button class="btn btn-secondary">Ver detalles</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                }
                else
                {
                    <p>No hay productos disponibles.</p>
                }
            </div>
        </div>


